<div style="margin: 50px">

<p>
  <strong>Store name:</strong>
  <%= @store.store_name %>
</p>

<p>
  <strong>Store phone:</strong>
  <%= @store.store_phone %>
</p>

<p>
  <strong>Company:</strong>
  <%= @store.company_id %>
</p>

<%= form_for([@company, @store]) do |f| %>
<%= f.fields_for :address do |address| %>
    <%= render :partial => 'shared/address', :locals => {:f => address} %>
<% end %>
<% end %>

<%= button_to 'Edit', edit_company_store_path(@company, @store), :method => :get %>
<br>
<%= button_to 'Back', company_stores_path(@store.company), :method => :get %>
</div>
